{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:default",
    "store:default",
    "log:default",
    "http:default",
    "os:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**:**/api2/json/:endpoint+"
        }
      ]
    }
  ]
}
